/* pages/analyse/analyse.wxss */
.message_list {}
.message_item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 120rpx;
  padding: 10rpx 20rpx;
  border-bottom: 1px solid #eaeaea;
  background: #fcfcfc;
}
.message_item>view:first-child {
  width: 670rpx;
}
.message_item>view:first-child view:first-child {
  /* font-size: 16px; */
  color: #333;
  line-height: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.message_item>view:first-child view:last-child {
  color: #999;
  line-height: 40rpx;
}
.message_item>view:last-child {
  color: #999;
}
.more {
  line-height: 80rpx;
  color: #999;
  text-align: center;
}