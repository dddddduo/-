.container {
  background: #fff;
  font-size: 14px;
}
.show_section {
  padding: 0 20rpx;
  margin-bottom: 40rpx;
}
.show_section .title_line {
  min-height: 100rpx;
  display: flex;
  justify-content: space-between;
}
.title_line text:first-child {
  width: 200rpx;
  height: 100%;
  line-height: 100rpx;
}
.title_line text:last-child {
  width: 500rpx;
  word-break: break-all;
  line-height: 100rpx;
}
.title_section {
  margin-top: 20rpx;
  line-height: 60rpx;
}
.section_line {
  line-height: 60rpx;
}
