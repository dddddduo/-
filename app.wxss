/**app.wxss**/
page {
  height: 100%;
}
@import './assets/font/iconfont.wxss';
.container {
  min-height: 100%;
  box-sizing: border-box;
  font-size: 14px;
  background: #f4f4f4;
}
.null {
  padding: 75rpx 0;
  color: #999;
  font-size: 16px;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.null image {
  width: 240rpx;
  height: 240rpx;
}
.click_class {
  opacity: 0.67;
}
.ul-title {
  padding: 0 40rpx 10rpx 16rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-weight: bold;
  color: rgba(102, 102, 102, 1);
}
.agent_list {
  margin: 0 30rpx;
  border: 1px solid #eaeaea;
}
.agent_item {
  display: flex;
  border-bottom: 1px solid #eaeaea;
  height: 60rpx;
  font-size: 14px;
}
.agent_item view {
  height: 100%;
  line-height: 60rpx;
  text-align: center;
}
.agent_item view:first-child {
  width: 40%;
}
.agent_item view:last-child {
  width: 60%;
}
.agent_item:last-of-type {
  border: none;
}
.atta_list {
  margin-bottom: -30rpx;
  padding: 0 30rpx;
  display: flex;
  flex-wrap: wrap;
  /* justify-content: space-around; */
}
.atta_list .atta_item {
  width: 200rpx;
  height: 200rpx;
  margin-bottom: 30rpx;
  margin-right: 38rpx;
  border: 1px solid #eaeaea;
}
.atta_list>view:nth-child(3n+3) {
  margin-right: 0;
}
.atta_list>view image {
  width: 200rpx;
  height: 200rpx;
}
.atta_list>view view {
  width: 200rpx;
  height: 200rpx;
  text-align: center;
  line-height: 200rpx;
  font-size: 14px;
}
.inner_null {
  line-height: 120rpx;
  text-align: center;
  color: #999;
  font-size: 14px;
  width: 100%;
  border: none;
}
.text_area {
  padding: 0 20rpx 30rpx;
}
.info {
  padding: 6rpx 10rpx;
  line-height: 60rpx;
  min-height: 180rpx;
  border: 1px solid #eaeaea;
  color: #666;
}
